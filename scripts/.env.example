# AgentVault Demo Agent Configuration
# Copy this file to .env and update the values

# =============================================================================
# REQUIRED - Agent Configuration
# =============================================================================

# Your agent's private key (Ed25519)
# Generate with: sui client new-address ed25519
# Export with: sui keytool export --key-identity <address>
AGENT_PRIVATE_KEY=

# The vault object ID to operate on
VAULT_ID=

# =============================================================================
# REQUIRED FOR SWAP EXECUTION
# =============================================================================

# DEEP token coin object ID (for paying DeepBook fees)
# Find your DEEP coins on Sui Explorer under your wallet
DEEP_COIN_ID=

# Set to 'true' to actually execute swaps (otherwise just validates)
EXECUTE_SWAP=false

# =============================================================================
# OPTIONAL - Network Configuration
# =============================================================================

SUI_RPC_URL=https://fullnode.testnet.sui.io:443
PACKAGE_ID=0x9eb66e8ef73279472ec71d9ff8e07e97e4cb3bca5b526091019c133e24a3b434

# DeepBook v3 Pool (defaults to DEEP/SUI testnet)
POOL_ID=0x0064034cf7f797e298bd9cd506f0e127ce511a798b3d9113e2f0cdb7e2c049f6
